eval "$(starship init bash)"

# Work directory alias
alias dev="cd ~/Code/Devs"
alias larap="cd ~/Code/Laravel"
alias rjs="cd ~/Code/ReactJS"
alias rn="cd ~/Code/ReactNative"

# Clear Screen Alias
alias cls="clear"

# Config Alias
alias openbash="notepad ~/.bash_profile"
alias sourcebash="source ~/.bash_profile"
alias starship="notepad ~/.config/starship.toml"

# Laravel Alias
alias serve="php artisan serve"
alias routelist="php artisan route:list"
alias migrate="php artisan migrate"
alias rollback="php artisan migrate:rollback"
alias model="php artisan make:model"
alias tinker="php artisan tinker"
